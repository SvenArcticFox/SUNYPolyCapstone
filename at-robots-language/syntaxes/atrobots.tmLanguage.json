{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "AT-Robots Assembly",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.atrobots",
				"match": "\\b((?i)jne|jge)\\b"
			}]
		},
		"operator": {
			"patterns": [{
				"name": "keyword.operator.atrobots",
				"match": "\\b((?i)ADD|INC|DEC|SHL|SHR|ROL|ROR|SAL|SAR|NEG|mod|CMP)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.atrobots",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.atrobots",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.at2"
}